@using Assignment9.Models

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>ViewGetUsers</title>
</head>
<body>
<div>
      <table>
          <tr>
              <th>id</th>
              <th>username</th>
              <th>password</th>
          </tr>
          @foreach (var user in (List<User>) ViewData["users"])
          {
              <tr>
                  <td>@user.Id</td>
                  <td>@user.Username</td>
                  <td>@user.Password</td>
              </tr>
          }
      </table>
</div>
</body>
</html>